{
   "Configuration":{
      "Appenders":{
         "Console":{
            "name":"STDOUT",
            "target":"SYSTEM_OUT",
            "PatternLayout":{
               "pattern":"%d %-5p [%t] %C{2} (%F:%L) - %m%n"
            }
         }
      },
      "Loggers":{
         "Logger":{
            "name":"org.apache.log4j.json",
            "level":"info"
         },
         "Root":{
            "level":"debug",
            "AppenderRef":{
               "ref":"STDOUT"
            }
         }
      }
   }
}